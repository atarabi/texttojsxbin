{
	"name": "texttojsxbin",
	"displayName": "TextToJSXBIN",
	"description": "Encode selected text to JSXBIN and put inside eval",
	"categories": ["Formatters", "Other", "Extension Packs"],
	"keywords" : [
		"ExtendScript", "JSXBIN", "JSX", "After Effects"
	],
	"publisher" : "motionland",
	"version": "0.0.2",
	"author" : {
		"name" : "Alex White"
	},
	"engines": {
		"vscode": "^1.34.0"
	},
	"activationEvents": [
		"onCommand:extension.texttojsxbin"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.texttojsxbin",
				"title": "TextToJSXBIN"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "extension.texttojsxbin",
					"group": "YourGroup@1"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"tslint": "^5.20.0",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28"
	},
	"repository": {
    "type": "git",
    "url": "https://github.com/axwt/texttojsxbin"
  },
  "homepage": "https://github.com/axwt/texttojsxbin/blob/master/README.md",
	"icon": "images/icon.png"
}